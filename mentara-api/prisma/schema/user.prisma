model User {
  id         String   @id @default(uuid())
  firstName  String
  middleName String
  lastName   String
  birthDate  DateTime
  address    String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  // Relations
  posts       Post[]
  comments    Comment[]
  memberships Membership[]

  Worksheet Worksheet[]
}
