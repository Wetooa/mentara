    }
    /**
     * Validate therapist exists and is active
     */
    async validateTherapistExists(therapistId) {
        const therapist = await this.prisma.therapist.findFirst({
            where: {
                userId: therapistId,
                status: 'APPROVED',
            },
            include: {
                user: {
                    select: {
                        isActive: true,
