exports.getCommunitiesByIllness = getCommunitiesByIllness;
exports.getAllIllnessTypes = getAllIllnessTypes;
exports.getRecommendedCommunitiesFromAI = getRecommendedCommunitiesFromAI;
exports.getCommunityRecommendationsWithScores = getCommunityRecommendationsWithScores;
