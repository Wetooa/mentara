    scoring: {
        scoreMapping: { 0: 0, 1: 1, 2: 2, 3: 3, 4: 4 },
        severityLevels: {
            low: { range: [0, 17], label: "Low-level burnout" },
            moderate: { range: [18, 29], label: "Moderate burnout" },
            high: { range: [30, 132], label: "High-level burnout" },
        },
        getScore: (answers) => {
            // Combine scores from all three scales for a total burnout score
            // Emotional exhaustion: items 0-6
            const emotionalExhaustion = answers
                .slice(0, 7)
                .reduce((sum, answer) => sum + (answer || 0), 0);
            // Depersonalization: items 7-13
            const depersonalization = answers
                .slice(7, 14)
                .reduce((sum, answer) => sum + (answer || 0), 0);
            // Personal achievement: items 14-21 (reverse scored)
            // Note: For personal achievement, higher scores indicate LOWER burnout
            // So we subtract from max possible (32) to convert to burnout score
            const personalAchievement = answers
                .slice(14, 22)
                .reduce((sum, answer) => sum + (answer || 0), 0);
            // For total burnout score, we could use a weighted approach or simply sum them
            return (emotionalExhaustion + depersonalization + (32 - personalAchievement));
        },
        getSeverity: (score) => {
            if (score <= 17)
                return "Low-level burnout";
            if (score >= 18 && score <= 29)
                return "Moderate burnout";
            if (score >= 30)
                return "High-level burnout";
            return "Invalid score";
        },
    },
    disclaimer: "This questionnaire is a screening tool to assess burnout risk. It does not serve as a diagnostic tool and should be followed by a clinical evaluation. If you are experiencing symptoms of burnout, please seek support from a healthcare professional.",
