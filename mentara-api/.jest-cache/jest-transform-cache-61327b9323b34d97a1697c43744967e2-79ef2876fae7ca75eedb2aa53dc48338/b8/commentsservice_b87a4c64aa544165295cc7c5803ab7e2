          hearted: false
        };
      } else {
        /* istanbul ignore next */
        cov_k7yujkxez().b[48][1]++;
        cov_k7yujkxez().s[79]++;
        // Add heart
        await this.prisma.commentHeart.create({
          data: {
            commentId,
            userId
          }
        });
