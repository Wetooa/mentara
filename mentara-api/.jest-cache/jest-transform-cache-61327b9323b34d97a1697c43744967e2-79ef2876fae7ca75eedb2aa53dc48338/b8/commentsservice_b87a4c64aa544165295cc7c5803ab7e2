  // File attachments are now handled directly with attachment arrays
  // No separate file attachment models needed
  /**
   * Report a comment for inappropriate content
   */
  async reportComment(commentId, reporterId, reason, content) {
    /* istanbul ignore next */
    cov_k7yujkxez().f[13]++;
    cov_k7yujkxez().s[90]++;
    try {
      // Verify comment exists
      const comment =
      /* istanbul ignore next */
      (cov_k7yujkxez().s[91]++, await this.prisma.comment.findUnique({
        where: {
          id: commentId
        }
      }));
      /* istanbul ignore next */
      cov_k7yujkxez().s[92]++;
      if (!comment) {
        /* istanbul ignore next */
        cov_k7yujkxez().b[55][0]++;
        cov_k7yujkxez().s[93]++;
        throw new common_1.NotFoundException('Comment not found');
      } else
      /* istanbul ignore next */
      {
        cov_k7yujkxez().b[55][1]++;
      }
      // Check if user already reported this comment
      const existingReport =
      /* istanbul ignore next */
      (cov_k7yujkxez().s[94]++, await this.prisma.report.findFirst({
        where: {
          commentId,
          reporterId
        }
      }));
      /* istanbul ignore next */
      cov_k7yujkxez().s[95]++;
      if (existingReport) {
        /* istanbul ignore next */
        cov_k7yujkxez().b[56][0]++;
        cov_k7yujkxez().s[96]++;
        throw new common_1.ForbiddenException('You have already reported this comment');
      } else
      /* istanbul ignore next */
      {
        cov_k7yujkxez().b[56][1]++;
      }
      // Create the report
      const report =
      /* istanbul ignore next */
      (cov_k7yujkxez().s[97]++, await this.prisma.report.create({
        data: {
          commentId,
          reporterId,
          reason,
          content,
          status: 'PENDING'
        }
      }));
      /* istanbul ignore next */
      cov_k7yujkxez().s[98]++;
      return report.id;
    } catch (error) {
      /* istanbul ignore next */
      cov_k7yujkxez().s[99]++;
      if (
      /* istanbul ignore next */
      (cov_k7yujkxez().b[58][0]++, error instanceof common_1.NotFoundException) ||
      /* istanbul ignore next */
      (cov_k7yujkxez().b[58][1]++, error instanceof common_1.ForbiddenException)) {
        /* istanbul ignore next */
