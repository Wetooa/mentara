    }
  }
  async sendPasswordResetConfirmationEmail(email) {
    /* istanbul ignore next */
    cov_2khujb3ztu().f[8]++;
    cov_2khujb3ztu().s[86]++;
    try {
      const html =
      /* istanbul ignore next */
