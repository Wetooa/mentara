      cov_2khujb3ztu().b[32][1]++;
    }
    // Check for at least one uppercase letter
    cov_2khujb3ztu().s[69]++;
    if (!/[A-Z]/.test(password)) {
      /* istanbul ignore next */
      cov_2khujb3ztu().b[33][0]++;
      cov_2khujb3ztu().s[70]++;
      throw new common_1.BadRequestException('Password must contain at least one uppercase letter');
    } else
    /* istanbul ignore next */
    {
      cov_2khujb3ztu().b[33][1]++;
    }
    // Check for at least one lowercase letter
    cov_2khujb3ztu().s[71]++;
    if (!/[a-z]/.test(password)) {
      /* istanbul ignore next */
      cov_2khujb3ztu().b[34][0]++;
      cov_2khujb3ztu().s[72]++;
      throw new common_1.BadRequestException('Password must contain at least one lowercase letter');
    } else
    /* istanbul ignore next */
    {
      cov_2khujb3ztu().b[34][1]++;
    }
    // Check for at least one number
    cov_2khujb3ztu().s[73]++;
    if (!/\d/.test(password)) {
      /* istanbul ignore next */
      cov_2khujb3ztu().b[35][0]++;
      cov_2khujb3ztu().s[74]++;
      throw new common_1.BadRequestException('Password must contain at least one number');
    } else
    /* istanbul ignore next */
    {
      cov_2khujb3ztu().b[35][1]++;
    }
    // Check for at least one special character
    cov_2khujb3ztu().s[75]++;
    if (!/[!@#$%^&*()_+\-=\[\]{};':"\\|,.<>\/?]/.test(password)) {
      /* istanbul ignore next */
      cov_2khujb3ztu().b[36][0]++;
      cov_2khujb3ztu().s[76]++;
      throw new common_1.BadRequestException('Password must contain at least one special character');
    } else
    /* istanbul ignore next */
    {
      cov_2khujb3ztu().b[36][1]++;
    }
    // Check for common weak passwords
    const commonPasswords =
    /* istanbul ignore next */
    (cov_2khujb3ztu().s[77]++, ['password', 'password123', '123456', 'qwerty', 'abc123', 'letmein', 'welcome', 'monkey', '1234567890', 'admin']);
    /* istanbul ignore next */
    cov_2khujb3ztu().s[78]++;
    if (commonPasswords.includes(password.toLowerCase())) {
      /* istanbul ignore next */
      cov_2khujb3ztu().b[37][0]++;
      cov_2khujb3ztu().s[79]++;
      throw new common_1.BadRequestException('Password is too common. Please choose a more secure password');
    } else
    /* istanbul ignore next */
    {
      cov_2khujb3ztu().b[37][1]++;
    }
  }
  async sendPasswordResetEmail(email, firstName, token) {
    /* istanbul ignore next */
    cov_2khujb3ztu().f[7]++;
    const resetUrl =
    /* istanbul ignore next */
    (cov_2khujb3ztu().s[80]++, `${process.env.FRONTEND_URL}/reset-password?token=${token}`);
    /* istanbul ignore next */
    cov_2khujb3ztu().s[81]++;
    try {
      const html =
      /* istanbul ignore next */
      (cov_2khujb3ztu().s[82]++, `
