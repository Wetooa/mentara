    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.3",
    "@prisma/client": "^6.19.0",
    "@types/bcrypt": "^5.0.2",
    "@types/multer": "^1.4.12",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
