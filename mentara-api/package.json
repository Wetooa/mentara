{
  "name": "mentara-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r tsconfig-paths/register dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "NODE_OPTIONS='--inspect-brk' jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:add": "ts-node prisma/seed.ts -- --add",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "assign-therapist": "ts-node scripts/assign-therapist.ts",
    "assign-random-therapists": "ts-node scripts/assign-therapist.ts random",
    "validate-endpoints": "ts-node scripts/validate-api-endpoints.ts",
    "kill-port": "npx kill-port 3001",
    "check-port": "lsof -i tcp:3001 || echo 'Port 3001 is available'",
    "dev:clean": "npm run kill-port && npm run start:dev"
  },
  "dependencies": {
    "@emailjs/nodejs": "^5.0.2",
