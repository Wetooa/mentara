{
    "name": "mentara-landing",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/landing/src",
    "tags": [
        "type:app",
        "scope:landing"
    ],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vite build",
                "cwd": "apps/landing"
            }
        },
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vite dev",
                "cwd": "apps/landing"
            }
        },
        "preview": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vite preview",
                "cwd": "apps/landing"
            }
        },
        "check": {
            "executor": "nx:run-commands",
            "options": {
                "command": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
                "cwd": "apps/landing"
            }
        }
    }
}